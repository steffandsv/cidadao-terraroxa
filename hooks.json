[
  {
    "id": "redeploy-cidadao",
    "execute-command": "/var/scripts/redeploy.sh",
    "command-working-directory": "/app",
    "trigger-rule": {
      "match": {
        "type": "payload-hmac-sha1",
        "secret": "MABUS_SUPER_SECRET",
        "parameter": {
          "source": "header",
          "name": "X-Hub-Signature"
        }
      }
    }
  }
]